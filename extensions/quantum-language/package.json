{
  "name": "quantum-language",
  "displayName": "Quantum Language",
  "description": "Syntax highlighting and language basics for Quantum (.sa) files",
  "version": "0.0.1",
  "publisher": "local",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://example.com/quantum-language.git"
  },
  "license": "MIT",
  "scripts": {
    "package": "npx @vscode/vsce package --allow-missing-repository"
  },
  "main": "./extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "languages": [
      {
        "id": "quantum",
        "aliases": [
          "Quantum",
          "quantum"
        ],
        "extensions": [
          ".sa"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "quantum",
        "scopeName": "source.quantum",
        "path": "./syntaxes/quantum.tmLanguage.json"
      }
    ]
  },
  "files": [
    "extension.js",
    "language-configuration.json",
    "syntaxes/quantum.tmLanguage.json",
    "README.md"
  ]
}
