
let age;
let height;
let grade;

printf("Enter your age: ");
scanf("%d", &age);

printf("Enter your height: ");
scanf("%f", &height);

printf("Enter your grade: ");
scanf(" %c", &grade);   // space before %c avoids newline issue

printf("\n--- Your Data ---\n");
printf("Age: %d\n", age);
printf("Height: %.2f\n", height);
printf("Grade: %c\n", grade);